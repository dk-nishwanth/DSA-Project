{
  "name": "codespace-backend",
  "version": "1.0.0",
  "description": "Backend service for code execution in Docker containers",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t codespace-backend .",
    "docker:run": "docker run -p 3001:3001 -v /var/run/docker.sock:/var/run/docker.sock codespace-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "dockerode": "^4.0.0",
    "tar-stream": "^3.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/dockerode": "^3.3.23",
    "@types/tar-stream": "^3.1.3",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": [
    "code-execution",
    "docker",
    "sandbox",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT"
}